<p id="notice"><%= notice %></p>

<h1>Trendings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trendings.each do |trending| %>
      <tr>
        <td><%= link_to 'Show', trending %></td>
        <td><%= link_to 'Edit', edit_trending_path(trending) %></td>
        <td><%= link_to 'Destroy', trending, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trending', new_trending_path %>
